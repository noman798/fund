#! /usr/bin/env coffee
yargs = require('yargs')
argv = yargs
.command(
    'rule',
    '获取安全规则',
    ->
        console.log "!!!"
)
.command(
    'rule push',
    '上传安全规则',
    ->
        console.log "!!! push"
)
.command(
    'rule pull',
    '下载安全规则',
    ->
        console.log "!!! pull"
)
.help('h')
.alias('h', 'help').argv

if not argv._.length
    yargs.showHelp()
